{
  "version": 2,
  "buildCommand": "npm run build",
  "outputDirectory": "dist",
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "/api/$1"
    },
    {
      "src": "/assets/(.*)",
      "dest": "/assets/$1"
    },
    {
      "src": "/favicon_jim_e",
      "dest": "/favicon_jim_e"
    },
    {
      "src": "/jim_e_brown_website.jpeg",
      "dest": "/jim_e_brown_website.jpeg"
    },
    {
      "src": "/(.*)",
      "dest": "/index.html"
    }
  ]
}
